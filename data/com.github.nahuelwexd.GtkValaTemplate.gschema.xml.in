<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

<schema path="/com/github/nahuelwexd/GtkValaTemplate/" id="@APP_ID@" gettext-domain="@GETTEXT_PACKAGE@">

  <key name="window-height" type="i">
    <default>400</default>
    <summary>Window height</summary>
  </key>

  <key name="window-width" type="i">
    <default>600</default>
    <summary>Window width</summary>
  </key>

  <key name="is-maximized" type="b">
    <default>false</default>
    <summary>Whether the window is maximized or not</summary>
  </key>

</schema>

</schemalist>
